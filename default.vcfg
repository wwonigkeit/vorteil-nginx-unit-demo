[[program]]
  args = "/usr/local/bin/docker-entrypoint.sh unitd --control 0.0.0.0:8080"
  env = ["PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin", "UNIT_VERSION=1.21.0-1~buster"]
  logfiles = ["/var/log/unit.log"]
  privilege = "superuser"

[[network]]
  tcp = ["80","8300","8080","8888"]

[system]
  hostname = "nginx-unit"
  user = "root"

[vm]
  ram = "256 MiB"
  disk-size = "+256 MiB"

[[logging]]
  config = ["Name=http", "Host=35.201.9.144", "Port=3080", "Format=json"]
  type = "system"

[[logging]]
  config = ["Name=http", "Host=35.201.9.144", "Port=3080", "Format=json"]
  type = "kernel"
